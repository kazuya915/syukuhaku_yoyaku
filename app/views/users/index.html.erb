<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>


<h2>ユーザー一覧</h2>

<% @users.each do |user| %>
    <table>
        <tr>
            <th>ユーザー名</th>
            <td>
                
                    <%= user.user_name %>
                
            </td>
        </tr>
        <tr>
            <th>メールアドレス</th>
            <td>
               
                    <%= user.email %>
                
            </td>
        </tr>
        <tr>
            <th>introduction</th>
            <td>
               
                    <%= user.user_introduction %>
                
            </td>
        </tr>
    </table>
<% end %>


<%= link_to 'ログアウト', 
  destroy_user_session_path,
  class: 'dropdown-item',
  method: :delete
%>